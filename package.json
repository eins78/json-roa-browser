{
  "name": "json-roa-browser",
  "version": "2.0.0-alpha.1+madek3",
  "description": "JSON-ROA browser",
  "scripts": {
    "test": "npm run model-tests",
    "model-tests": "coffeetape spec/models/**.coffee",
    "integration-tests": "coffee ./spec/integration.coffee",
    "dev": "webpack-dev-server",
    "watch": "webpack -d  --watch --progress --colors",
    "build": "webpack -p --progress",
    "predev": "npm run -s build:clean",
    "prewatch": "npm run -s build:clean",
    "prebuild": "npm run -s build:clean",
    "build:clean": "rm -rf build && mkdir build",
    "ci:build": "webpack -p --json --no-color",
    "dist:madek": "NPMVERSION=$(node -p 'require(\"./package.json\").version') ; GITHASH=$(git log -n1 --format=\"%h\") ; VERSION=\"v${NPMVERSION}+g${GITHASH}\" ; npm run -s build && rsync -rv build/ tmp/dist-madek/ && cd tmp/dist-madek && git add . && git commit -m \"build for $VERSION\" && git push origin HEAD:refs/heads/$VERSION ; cd ../..",
    "predist:madek": "rm -rf tmp/dist-madek && mkdir -p tmp && git clone --depth=1 git@github.com:Madek/madek_api-browser_build.git tmp/dist-madek",
    "deploy:178": "npm run build && rsync -avPh --delete build/*{,.*} mt:domains/dev.178.is/html/json-roa-browser/",
    "tap-spec": "tap-spec",
    "coffee": "coffee",
    "selenium-standalone": "selenium-standalone"
  },
  "authors": [
    "Max F. Albrecht <1@178.is>",
    "Thomas Schank <DrTom@schank.ch>"
  ],
  "repository": "eins78/json-roa-browser",
  "license": "MIT",
  "dependencies": {
    "active-lodash": "^1.2.2",
    "ampersand-app": "^1.0.4",
    "ampersand-collection": "^1.6.1",
    "ampersand-model": "^6.0.2",
    "ampersand-react-mixin": "^1.0.1",
    "ampersand-state": "^5.0.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.3",
    "codemirror": "^5.12.0",
    "hashchange": "^1.0.0",
    "json-stringify-pretty-compact": "^1.0.1",
    "media-typer": "^0.3.0",
    "nets": "^3.2.0",
    "node-status-codes": "^2.0.0",
    "parse-headers": "^2.0.1",
    "qs": "^6.1.0",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.3",
    "react-codemirror": "^0.2.5",
    "react-dom": "^0.14.6",
    "semver": "^5.1.0",
    "uri-templates": "^0.1.9"
  },
  "devDependencies": {
    "@eins78/typopro-source-code-pro": "*",
    "@eins78/typopro-source-sans-pro": "*",
    "ansi_up": "^1.3.0",
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "coffee-jsx-loader": "^0.1.3",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "coffeetape": "^1.0.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "get-stdin": "^5.0.1",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "selenium-standalone": "^5.0.0",
    "style-loader": "^0.13.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.0",
    "url-loader": "^0.5.7",
    "webdriverio": "^4.0.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
